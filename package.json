    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "type-graphql-dataloader": "^0.5.0",
    "typeorm": "^0.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
